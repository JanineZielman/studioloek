{
  "format": "page",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "preview_image": {
        "type": "Image",
        "config": {
          "label": "Preview Image",
          "constraint": {
            "width": 1080
          },
          "thumbnails": []
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "meegewerkt_aan": {
        "type": "Boolean",
        "config": {
          "label": "Meegewerkt Aan",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "image": {
              "type": "SharedSlice"
            },
            "embed": {
              "type": "SharedSlice"
            },
            "hero_slider": {
              "type": "SharedSlice"
            },
            "rich_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
